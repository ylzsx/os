<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.os">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/os"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/os"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".bussiness.activity.MemoryInfoActivity"></activity>
        <activity android:name=".bussiness.activity.PasswordActivity" />
        <activity android:name=".bussiness.activity.BitMapActivity" />
        <activity android:name=".bussiness.activity.MainActivity" />
        <activity android:name=".bussiness.activity.ThreadInfoActivity"/>
        <activity android:name=".bussiness.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>